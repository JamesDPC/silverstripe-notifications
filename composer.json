{
  "name": "symbiote/silverstripe-notifications",
  "description": "Send CMS editable system notifications from code",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "notifications"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Shea Dawson",
      "email": "shea@symbiote.com.au"
    },
    {
      "name": "Marcus Nyeholt",
      "email": "marcus@symbiote.com.au"
    },
    {
      "name": "Nik Spijkerman",
      "email": "nspyke@github.com"
    }
  ],
  "require": {
    "silverstripe/framework": "^5",
    "symbiote/silverstripe-multivaluefield": "^6"
  },
  "autoload": {
    "psr-4": {
      "Symbiote\\Notifications\\": "src/",
      "Symbiote\\Notifications\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "installer-name": "notifications",
    "expose": [
      "images"
    ],
    "branch-alias": {
      "dev-master": "4.4.x-dev"
    }
  },
  "suggest": {
    "symbiote/silverstripe-queuedjobs": "Use the SendNotificationJob to queue and send notifications."
  },
  "replace": {
    "silverstripe-australia/notifications": "self.version"
  },
  "repositories": [
      {
          "type": "git",
          "url": "https://github.com/nswdpc/ci-files.git"
      }
  ],
  "require-dev": {
      "squizlabs/php_codesniffer": "^3.0",
      "cambis/silverstripe-rector": "^0.5.1",
      "phpunit/phpunit": "^9.5",
      "syntro/silverstripe-phpstan": "^5",
      "nswdpc/ci-files": "^1",
      "phpstan/phpstan": "~1.11.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
